<div class="user-sho container">

  <%= render "common/admin_profile_bar", user: @user_profile, page: "user-show" %>

  <div class="center">
    <div class="title--green"><%= @user_profile.user_name %>さんおすすめ</div>

    <div class="container__content">
      <% @youtubers.each do |youtuber| %>
        <%= link_to admin_youtuber_path(youtuber), class: "user-show__link" do %>

          <%= render "common/user_show", youtuber: youtuber %>

        <% end %>
      <% end %>
    </div>
  </div>

  <%= render "common/genre_search_bar", genres: @genres %>

</div>

<script>
$('.review-evaluation').empty();
  $('.review-evaluation').raty({
    readOnly: true,
    number: 5,
    score: function() {
      return $(this).attr('data-score');
    },
    path: '/assets/'
  });
</script>