<div class="genre-search">
  <div>ジャンル検索</div>
  <div>
    <%= link_to "ジャンルを追加+", new_genre_path %>
    <!--ジャンルごとのリンクを設定するのは後日-->
    <%= link_to "全てのジャンル", youtubers_path(sort: "genre-search") %>
    <% genres.each do |genre| %>
      <ul>
        <li>
          <%= link_to "#{genre.genre_name}", youtubers_path(id: genre.id ,sort: "genre-search") %>
        </li>
      </ul>
    <% end %>
  </div>
</div>